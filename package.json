  "description": "a typescript node tool for interpolating a template array with a series of plugins",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "start": "ts-node ./src/main.ts",
    "build": "tsc",
    "test": "npx jest"
  },
  "author": "Carl Burks",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "flow",
    "typescript"
  ],
  "directories": {
    "example": "examples"
  },
  "dependencies": {
   },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BurksBrand/interpolate.git"
